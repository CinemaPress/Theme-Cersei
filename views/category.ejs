<!DOCTYPE html>
<html lang="<%- page.language %>">
<head>

    <% include includes/head.ejs %>

</head>
<body>
<div class="kapla">

    <% include includes/header.ejs %>

    <div id="contenedor">
        <div class="contenido">

            <div class="box">

                <% include includes/header2.ejs %>

                <div class="box_item">

                    <% if (typeof slider === 'object' && slider.length) { %>

                    <div class="slider_box s_home">
                        <div class="head_slider">
                            <div class="controles">
                                <a class="prev btn"><b class="icon-angle-left"></b></a>
                                <a class="play btn"><b class="icon-playback-play"></b></a>
                                <a class="next btn"><b class="icon-angle-right"></b></a>
                            </div>
                        </div>
                        <div class="owl-carousel owl-theme slider1">

                            <% slider.forEach(function (movie) { %>

                            <div class="item">
                                <div class="imagens">
                                    <a href="<%- movie.url %>"><img src="<%- movie.poster %>" alt="<%= movie.title %> (<%- movie.year %>) полный фильм онлайн" style="width: 100%; height: 100%;"/></a>
                                    <span class="imdb"><b><b class="icon-star"></b></b> <%- movie.rating/10 %></span>
                                </div>
                                <span class="ttps" title="<%= movie.title %> (<%- movie.year %>)"><%= movie.title %> (<%- movie.year %>)</span>
                                <span class="ytps"><%- movie.year %></span>
                            </div>

                            <% }); %>

                        </div>
                    </div>

                    <% } %>

                    <div class="peliculas">

                        <% if (typeof top === 'object' && top.length) { %>

                        <div class="slider_box s_home">
                            <div class="head_slider">
                                <h3>
                                    Топ
                                </h3>
                                <div class="controles">
                                    <a class="prev btn"><b class="icon-angle-left"></b></a>
                                    <a class="play btn"><b class="icon-playback-play"></b></a>
                                    <a class="next btn"><b class="icon-angle-right"></b></a>
                                </div>
                            </div>
                            <div class="owl-carousel owl-theme slider1">

                                <% top.forEach(function (movie) { %>

                                <div class="item">
                                    <div class="imagens">
                                        <a href="<%- movie.url %>"><img src="<%- movie.poster %>" alt="<%= movie.title %> (<%- movie.year %>) полный фильм онлайн" style="width: 100%; height: 100%;"/></a>
                                        <span class="imdb"><b><b class="icon-star"></b></b> <%- movie.rating/10 %></span>
                                    </div>
                                    <span class="ttps" title="<%= movie.title %> (<%- movie.year %>)"><%= movie.title %> (<%- movie.year %>) полный фильм онлайн</span>
                                    <span class="ytps"><%- movie.year %></span>
                                </div>

                                <% }); %>

                            </div>
                        </div>

                        <% } %>

                        <div style="clear: both;"></div>

                        <h1><%= page.title %></h1>

                        <% if (typeof content === 'object') { %>
                        <div class="cinemapress-content">
                            <% if (content.image) { %>
                            <img src="<%- content.image %>" class="cinemapress-content-image">
                            <% } %>
                            <% if (content.description) { %>
                            <div class="cinemapress-content-description">
                                <%- content.description %>
                            </div>
                            <% } %>
                            <div class="cinemapress-content-publish">
                                <% if (content.tags) { %>
                                <span class="cinemapress-content-publish-tags"><img src="data:image/svg+xml;utf8;base64," style="width: 10px;"> <%- content.tags %></span>
                                <% } %>
                                <% if (content.tags) { %>
                                <span class="cinemapress-content-publish-publish"><img src="data:image/svg+xml;utf8;base64," style="width: 10px;"> <%- content.publish %></span>
                                <% } %>
                            </div>
                        </div>
                        <div style="clear: both;"></div>
                        <% } %>

                        <% if (page.seo) { %>
                        <div class="seo">
                            <%- page.seo %>
                        </div>
                        <% } %>

                        <div style="clear: both;"></div>

                        <% if (typeof page.sorting === 'object') { %>

                        <div class="pagination">

                            <% page.sorting.forEach(function (sort) { %>

                            <% if (sort.active) { %>

                            <a href="<%- sort.url %>" style="padding: 6px 4px;">
                                <strong style="color: #fff;"><%- sort.name %></strong>
                            </a>

                            <% } else { %>

                            <a href="<%- sort.url %>" style="padding: 6px 4px;">
                                <%- sort.name %>
                            </a>

                            <% } %>

                            <% }); %>

                        </div>

                        <% } %>

                        <div style="clear: both;"></div>

                        <% if (typeof movies === 'object' && movies.length) {
                            movies.forEach(function (movie) { %>

                        <div class="items">
                            <div class="item">
                                <a href="<%- movie.url %>">
                                    <div class="image">
                                        <img src="<%- movie.poster %>" alt="<%= movie.title %> (<%- movie.year %>) полный фильм" style="width: 100%; height: 100%;"/>
                                        <span class="player"></span>
                                        <span class="imdb"><b><b class="icon-star"></b></b> <%- movie.rating/10 %></span>
                                    </div>
                                </a>
                                <div class="fixyear">
                                    <h3 title="<%= movie.title %> (<%- movie.year %>)"><%= movie.title %> смотреть онлайн</h3>
                                    <span class="year"><%- movie.year %></span>
                                </div>
                                <% if (movie.quality) { %>
                                <span class="calidad2"><%- movie.quality %></span>
                                <% } %>
                            </div>
                        </div>

                        <% }); } %>

                        <div style="clear: both;"></div>

                        <% if (typeof page.pagination === 'object') { %>
                        <div class="pagination">
                            <% if (page.pagination.first) { %>
                            <a href="<%- page.pagination.first.link %>" rel="nofollow"><%- page.pagination.first.number %></a>
                            <span class="nav_ext">...</span>
                            <% } %>
                            <% if (page.pagination.prev.length) { %>
                            <% page.pagination.prev.forEach(function (prev) { %>
                            <a href="<%- prev.link %>" rel="nofollow"><%- prev.number %></a>
                            <% }); %>
                            <span class="nav_ext">...</span>
                            <% } %>
                            <span><%- page.pagination.current %></span>
                            <% if (page.pagination.next.length) { %>
                            <span class="nav_ext">...</span>
                            <% page.pagination.next.forEach(function (next) { %>
                            <a href="<%- next.link %>" rel="nofollow"><%- next.number %></a>
                            <% }); %>
                            <% } %>
                            <% if (page.pagination.last) { %>
                            <span class="nav_ext">...</span>
                            <a href="<%- page.pagination.last.link %>" rel="nofollow"><%- page.pagination.last.number %></a>
                            <% } %>
                        </div>
                        <% } %>

                        <div style="clear: both;"></div>

                        <% if (typeof content === 'object' && page.comments) { %>
                        <div style="padding: 10px;">
                            <%- page.comments %>
                        </div>
                        <% } %>

                    </div>

                    <% include includes/right.ejs %>

                </div>
            </div>

            <% include includes/footer.ejs %>

        </div>
    </div>
</div>
</body>
</html>