<div id="revel" class="skl">
    <div class="a">
        <% if (page.social) { %>
        <% if (page.social.vk) { %>
        <a class="roce" href="<%- page.social.vk %>" target="_blank">
            <b class="icon-vk"></b>
        </a>
        <% } %>
        <% if (page.social.facebook) { %>
        <a class="roce" href="<%- page.social.facebook %>" target="_blank">
            <b class="icon-facebook"></b>
        </a>
        <% } %>
        <% if (page.social.twitter) { %>
        <a class="roce" href="<%- page.social.twitter %>" target="_blank">
            <b class="icon-twitter"></b>
        </a>
        <% } %>
        <% if (page.social.gplus) { %>
        <a class="roce" href="<%- page.social.gplus %>" target="_blank">
            <b class="icon-google-plus"></b>
        </a>
        <% } %>
        <% if (page.social.youtube) { %>
        <a class="roce" href="<%- page.social.youtube %>" target="_blank">
            <b class="icon-youtube4"></b>
        </a>
        <% } %>
        <% if (page.social.instagram) { %>
        <a class="roce" href="<%- page.social.instagram %>" target="_blank">
            <b class="icon-profile"></b>
        </a>
        <% } %>
        <% } %>
        <div class="menus">
            <div>
                <ul>
                    <li class="page_item">
                        <a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>">Фильмы</a>
                    </li>
                    <li class="page_item">
                        <a href="/<%- page.urls.type %>/<%- page.urls.types.serial %>">Сериалы</a>
                    </li>
                    <li class="page_item">
                        <a href="/<%- page.urls.type %>/<%- page.urls.types.mult %>">Мультфильмы</a>
                    </li>
                    <li class="page_item">
                        <a href="/<%- page.urls.type %>/<%- page.urls.types.anime %>">Аниме</a>
                    </li>
                    <li class="page_item">
                        <a href="/<%- page.urls.type %>/<%- page.urls.types.tv %>">Передачи</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="b">
        <form method="get" action="/<%- page.urls.search %>/title">
            <div class="boxs">
                <input type="text" placeholder="Поиск ..." value="" name="q">
            </div>
        </form>
    </div>
</div>
<div class="header">
    <div>
        <ul>
            <li class="page_item">
                <a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>">Фильмы</a>
            </li>
            <li class="page_item">
                <a href="/<%- page.urls.type %>/<%- page.urls.types.serial %>">Сериалы</a>
            </li>
            <li class="page_item">
                <a href="/<%- page.urls.type %>/<%- page.urls.types.mult %>">Мультфильмы</a>
            </li>
            <li class="page_item">
                <a href="/<%- page.urls.type %>/<%- page.urls.types.anime %>">Аниме</a>
            </li>
            <li class="page_item">
                <a href="/<%- page.urls.type %>/<%- page.urls.types.tv %>">Передачи</a>
            </li>
        </ul>
    </div>
    <div class="buscador" style="padding: 1px 19px 1px 10px;">
        <form method="get" action="/<%- page.urls.search %>/title">
            <div class="imputo">
                <input class="buscar" placeholder="Поиск ..." name="q" type="text">
                <input value="Найти" style="background: none; border: 0; color:#999;cursor: pointer;" type="submit">
            </div>
        </form>
    </div>
</div>

<% if (typeof page.adv === 'object' && page.adv.top) { %>
<div style="clear: both;"></div>
<div style="text-align: center; margin: 10px auto">
    <%- page.adv.top %>
</div>
<div style="clear: both;"></div>
<% } %>